<p id="notice"><%= notice %></p>

<h1>Proyecto Programas</h1>

<table>
  <thead>
    <tr>
      <th>Id instituto</th>
      <th>Nombre</th>
      <th>Descripcion</th>
      <th>Id status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @proyecto_programas.each do |proyecto_programa| %>
      <tr>
        <td><%= proyecto_programa.id_instituto %></td>
        <td><%= proyecto_programa.nombre %></td>
        <td><%= proyecto_programa.descripcion %></td>
        <td><%= proyecto_programa.id_status %></td>
        <td><%= link_to 'Show', proyecto_programa %></td>
        <td><%= link_to 'Edit', edit_proyecto_programa_path(proyecto_programa) %></td>
        <td><%= link_to 'Destroy', proyecto_programa, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Proyecto Programa', new_proyecto_programa_path %>
