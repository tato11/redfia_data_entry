<p id="notice"><%= notice %></p>

<h1>Subcuencas</h1>

<table>
  <thead>
    <tr>
      <th>Id cuenca</th>
      <th>Nombre</th>
      <th>Id status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @subcuencas.each do |subcuenca| %>
      <tr>
        <td><%= subcuenca.id_cuenca %></td>
        <td><%= subcuenca.nombre %></td>
        <td><%= subcuenca.id_status %></td>
        <td><%= link_to 'Show', subcuenca %></td>
        <td><%= link_to 'Edit', edit_subcuenca_path(subcuenca) %></td>
        <td><%= link_to 'Destroy', subcuenca, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Subcuenca', new_subcuenca_path %>
