<p id="notice"><%= notice %></p>

<h1>Microcuencas</h1>

<table>
  <thead>
    <tr>
      <th>Id subcuenca</th>
      <th>Nombre</th>
      <th>Id status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @microcuencas.each do |microcuenca| %>
      <tr>
        <td><%= microcuenca.id_subcuenca %></td>
        <td><%= microcuenca.nombre %></td>
        <td><%= microcuenca.id_status %></td>
        <td><%= link_to 'Show', microcuenca %></td>
        <td><%= link_to 'Edit', edit_microcuenca_path(microcuenca) %></td>
        <td><%= link_to 'Destroy', microcuenca, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Microcuenca', new_microcuenca_path %>
