<p id="notice"><%= notice %></p>

<h1>Proyecto Programas</h1>

<table>
  <thead>
    <tr>
      <th>Id instituto</th>
      <th>Nombre</th>
      <th>Descripcion</th>
      <th>Id status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @proyectos.each do |proyecto| %>
      <tr>
        <td><%= proyecto.id_instituto %></td>
        <td><%= proyecto.nombre %></td>
        <td><%= proyecto.descripcion %></td>
        <td><%= proyecto.id_status %></td>
        <td><%= link_to 'Show', proyecto %></td>
        <td><%= link_to 'Edit', edit_proyecto_path(proyecto) %></td>
        <td><%= link_to 'Destroy', proyecto, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Proyecto Programa', new_proyecto_path %>
